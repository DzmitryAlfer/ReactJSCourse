{
    "extends": [
        "eslint:recommended",
		"airbnb"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module"
    },
  "plugins": ["react"],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
  "rules":{
    "linebreak-style": ["error", "windows"],
    "indent": ["warn", 4, { "SwitchCase": 1 }],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2
  }
}